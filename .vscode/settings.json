{
  "eslint.format.enable": true,
  "eslint.codeActionsOnSave.rules": null,
  "eslint.validate": ["javascript", "typescript", "html"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "editor.wordWrap": "on",
  "files.associations": {
    "*.css": "tailwindcss",
    "*.scss": "tailwindcss",
  }
}
